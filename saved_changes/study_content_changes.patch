+              </div>
+              <p className="text-xs text-gray-400">
+                Personalized AI Study Assistant  {format(new Date(), 'MMMM d, yyyy')}
+              </p>
+            </div>
+          </div>
+        </div>
+      </div>
+    </div>
+  )
+}
